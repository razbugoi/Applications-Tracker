# Supabase-powered environment template
# Copy to .env.local or configure in Vercel
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
# Server-side only variables (configure in Vercel project settings)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_DB_URL=postgresql://postgres:your-password@db.supabase.co:5432/postgres
SUPABASE_JWT_SECRET=your-jwt-secret
SUPABASE_DEFAULT_TEAM_ID=00000000-0000-0000-0000-000000000001
# Optional: disable Supabase Auth for local smoke tests (do not set in production)
NEXT_PUBLIC_SUPABASE_BYPASS_AUTH=false
